{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "***",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "DefaultConnection": "***"
  },
  "AutoMessagesSettings": {
    "SendingTimer": 10000,
    "MessageText": "***",
    "CommandMessageText" : "***"
  },
  "AllowedHosts": "*"
}
